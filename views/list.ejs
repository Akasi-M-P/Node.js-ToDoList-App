
<%- include('header'); -%>



    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1>
    </div>

    <div class="box">
        <form class="item" action="/" method="POST">
            <input type="text" name="addToList" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %> > + </button>
        
        
        </form>
   

   
        <% for(let i = 0; i< newItems.length; i++) { %>
        <div class="item">
            <input type="checkbox">

             <p> <%= newItems[i] %> </p>
            </div>

        <% } %>

       
    </div>
   
  
  
    <%- include('footer'); -%>


